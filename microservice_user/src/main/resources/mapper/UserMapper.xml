<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace = "com.user.mapper.UserMapper">
    <select id = "selectAllUser" resultType = "com.example.entity.user.User">
      select * from user;
    </select>

    <select id="queryUser" resultType="com.example.entity.user.User">
        select * from user where id = #{id};
    </select>
</mapper>